@{
    ViewData["Title"] = "Duplicado";
    
}


<div class="card">
        <div class="card-body">
    
         <form method="post">
           
            <div class="form-group row">
                <label for="numeros" class="col-sm-2 col-form-label">Lista de Número</label>
                <div class="col-sm-8">
                 <input type="text" readonly class="form-control-plaintext" id="numeros" value="10,34,44,667,789,2,5,852,22,34,78,94,9857,0,6">
            </div>
           

         </form>


        </div>
</div>


<div class="text-center">
   

   

<table class="table">
  <thead>
    <tr>
        <th scope="col">Elementos</th>
    </tr>
  </thead>
  <tbody>
       
        @foreach (var item in ViewBag.minha_lista)
        {
        <tr> <th>@item</th> </tr>
        }

  </tbody>
</table>

<div class="card" style="background-color:#3399ff; ">
<div class="card-body">
    <p style="color:#fff;">O primeiro item duplicado na lista é  @ViewBag.teste </p>
</div>
</div>



</div>

